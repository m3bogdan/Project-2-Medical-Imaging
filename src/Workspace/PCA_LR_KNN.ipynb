    "import numpy as np\n",
    "from sklearn.model_selection import cross_val_predict\n",
    "from sklearn.linear_model import LogisticRegression as LR\n",
    "from sklearn.neighbors import KNeighborsClassifier as KNN\n",
    "from sklearn.tree import DecisionTreeClassifier as DTC\n",
    "from sklearn.metrics import f1_score, confusion_matrix, precision_score, recall_score\n",
    "from sklearn.preprocessing import Binarizer\n",
    "\n",
    "# Let's assume X, Y are your features and target labels\n",
    "\n",
    "# Instantiate your classifiers\n",
    "classifiers = [LR(), KNN(), DTC()]  \n",
    "\n",
    "# Define a binarizer to convert probabilities to classes based on a threshold\n",
    "binarizer = Binarizer(threshold=0.5)\n",
    "\n",
    "# Perform cross-validation for each classifier\n",
    "for classifier in classifiers:\n",
    "    # Fit classifier\n",
    "    classifier.fit(X_train, Y_train)\n",
    "    \n",
    "    # Predict probabilities using cross_val_predict with 'predict_proba' as method\n",
    "    y_scores = cross_val_predict(classifier, X, Y, cv=7, method='predict_proba')\n",
    "\n",
    "    # y_scores are probabilities for both classes. If you're interested in probabilities of positive class\n",
    "    y_scores_pos = y_scores[:, 1]\n",
    "\n",
    "    # Convert probabilities to classes using binarizer\n",
    "    y_pred = binarizer.fit_transform(y_scores_pos.reshape(-1, 1))\n",
    "    y_pred = y_pred.ravel()  # Flatten to 1D\n",
    "\n",
    "    # Calculate evaluation metrics\n",
    "    f1 = f1_score(Y, y_pred)\n",
    "    precision = precision_score(Y, y_pred)\n",
    "    recall = recall_score(Y, y_pred)\n",
    "    tn, fp, fn, tp = confusion_matrix(Y, y_pred).ravel()\n",
    "    specificity = tn / (tn + fp)\n",
    "\n",
    "    print(f\"Classifier: {type(classifier).__name__}\")\n",
    "    print(f\"Probabilities: {y_scores_pos}\")\n",
    "    print(f\"F1 score: {f1}\")\n",
    "    print(f\"Precision: {precision}\")\n",
    "    print(f\"Recall/Sensitivity: {recall}\")\n",
    "    print(f\"Specificity: {specificity}\")\n",
    "    print(\"Confusion Matrix:\")\n",
    "    print(confusion_matrix(Y, y_pred))\n",
    "    print()\n"
